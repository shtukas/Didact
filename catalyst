#!/usr/bin/ruby

# encoding: UTF-8

require_relative "Libs/loader.rb"

# ----------------------------------------------------------------------------

Thread.new {
    loop {
        sleep 300
        system("#{File.dirname(__FILE__)}/vienna-import")
    }
}

loop {
    domain = Domain::getCurrentDomain(domain)
    ns16s = NS16sOperator::ns16s(domain)
    UIServices::mainView(domain, ns16s)
}
