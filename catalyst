#!/usr/bin/ruby

# encoding: UTF-8

require_relative "Libs/loader.rb"

# ----------------------------------------------------------------------------

AFewNx50s::initialise(false)

Thread.new {
    AFewNx50s::initialise(true)
}

Thread.new {
    loop {
        sleep 300
        system("#{File.dirname(__FILE__)}/vienna-import")
    }
}

Thread.new {
    loop {
        sleep 1200
        KeyValueStore::set(nil, "DE7C7BBC-845D-4511-A671-6B3E03BB75AC", AllTheNx50s::nx50s().size)
    }
}

TerminalDisplayOperator::displayLoop()
